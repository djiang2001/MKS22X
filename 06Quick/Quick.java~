import java.util.*;
public class Quick{

    public static int partition(int[] data, int start, int end){
	Random rand = new Random();
	int index = rand.nextInt(data.length);
	int pivot = data[index];
	int small = start + 1;
	int large = end - 1;
	swap(data,index,small);
	for(int i = small; i < end; i++){
	    if(data[i] <= pivot){
		swap(data, i, small);
		small++;
	    }
	    swap(data, small, large);
	    return large;
		}
    }

    public static int quickSelect(int[] data, int k){
	int result = part(data,0,data.length - 1);
	int start = 0;
	int e = data.length - 1;
	while(result  + 1 != k){
	    if(result + 1 > k){
		e = result  - 1;
		first = partition(data,start,e);
	    }
	    if(first + 1 < k){
		s = result + 1;
		result = partition(data,start,e);
	    }
	}
	return data[];
    }
    
    private static void swap(int[] data, int a, int b){
	int temp = data[a];
	data[a] = data[b];
	data[b] = temp;
    }

    public static String toString(int[] data){
	String result = "[";
	for (int i = 0; i < data.length; i++){
	    str += data[i];
	    if (i < data.length - 1){
		str += ", ";
	    }
	}
	result += "]";
	return result;
    }
    
    public static void main(String[] args){
	int[] test = {-100,999,100,4,1,0,3,2,999,999,999};
	System.out.println(partition(test, 0, test.length-1));
    }
}
